{
  "name": "@transferwise/neptune-css",
  "description": "Neptune CSS is TransferWise's Design System CSS library",
  "version": "0.8.9",
  "license": "MIT",
  "repository": {
    "type": "git",
    "fullname": "transferwise/neptune",
    "url": "git+https://github.com/transferwise/neptune.git"
  },
  "keywords": [],
  "author": {
    "name": "TransferWise",
    "url": "https://transferwise.com/"
  },
  "bugs": {
    "url": "https://github.com/transferwise/neptune/issues"
  },
  "homepage": "https://transferwise.github.io/neptune",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "test": "echo 'No tests defined'",
    "build": "npm-run-all build:*",
    "build:clean": "rm -rf dist",
    "build:copy-assets": "npm-run-all --parallel copy:*",
    "build:compile-legacy-bootstrap": "gulp lessCompiler --prod",
    "build:compile-postcss": "gulp postcssCompiler --prod",
    "dev": "npm-run-all dev:*",
    "dev:watch-postcss": "gulp postcssWatcher --dev",
    "copy:images": "cpx 'src/img/**' dist/img",
    "copy:fonts": "cpx 'src/fonts/*' dist/fonts",
    "lint": "npm-run-all lint:*",
    "lint:prettier": "prettier --find-config-path --write src/less/*.less",
    "pre-release-check": "release-to-github-with-changelog-pre-release-checks",
    "release-to-github": "release-to-github-with-changelog"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@transferwise/icons": "^0.14.0",
    "bootstrap": "github:transferwise/bootstrap#v5.18.15",
    "transferwise-iconfont": "transferwise/iconfont"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "cpx": "^1.5.0",
    "cssnano": "^4.1.10",
    "cssstats": "^3.4.0",
    "gulp": "^4.0.2",
    "gulp-cached": "^1.1.1",
    "gulp-dependents": "^1.2.3",
    "gulp-filter": "^6.0.0",
    "gulp-less": "^4.0.1",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-print": "^5.0.2",
    "gulp-rename": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.18",
    "postcss-custom-media": "^7.0.8",
    "postcss-custom-properties": "^9.0.2",
    "postcss-import": "^12.0.1",
    "postcss-nesting": "^7.0.1",
    "prettier": "^1.18.2",
    "release-to-github-with-changelog": "^1.2.2",
    "yargs": "^14.0.0"
  },
  "peerDependencies": {
    "@transferwise/eslint-config": "^2.1.0"
  },
  "prettier": "@transferwise/eslint-config/.prettierrc.js"
}
